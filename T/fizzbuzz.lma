func fizzbuzz(x, max) {
    match (x mod 3, x mod 5)
    (0, 0) {
        cout("fizzbuzz\n")
    }
    (0, _) {
        cout("fizz\n");
    }
    (_, 0) {
        cout("buzz\n")
    }
    (_, _) {
        cout("$x\n")
    }
    /match

    fizzbuzz(x+1, max)
}


func main() {
    fizzbuzz(1, 100)
}
