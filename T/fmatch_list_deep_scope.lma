
func foo(a: [#])
|([]) -> cout("done\n")
|(#c;t) ->
    cout("found c.\n")
    foo(t)
|(h;t) ->
    match h
    |#a -> cout("found a! what else is in $t ?\n")
    |#b -> cout("found b! what else is in $t ?\n")
    |#c -> cout("found c! what else is in $t ?\n")
    --
    foo(t)
--

func main() -> foo([#a, #b, #c]) --
